{
  "version": 2,
  "builds": [
    {
      "src": "final-project/manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "final-project/manage.py"
    }
  ],
  "functions": {
    "final-project/manage.py": {
      "runtime": "python3.9"
    }
  },
  "installCommand": "pip install -r requirements.txt"
}
