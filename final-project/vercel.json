{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "final_pro.settings"
  },
  "buildCommand": "python manage.py collectstatic --noinput"
}
